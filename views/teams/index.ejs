<%- include("../partials/header") %>
<h2 id="allTeams">ALL TEAMS</h2>
<table>
  <thead>
    <tr>
      <th>Team Name</th>
    </tr>
  </thead>
  <tbody>
    <% teams.forEach(function(t) { %>
    <tr>
      <td><a href="/teams/<%= t._id %>"><%= t.teamName %></a></td>
    </tr>
    <% }) %>
  </tbody>
</table>

<%- include("../partials/footer") %>